{
    "name": "yanhaoli/office-building",
    "description": "office building is a multi tenancy package for laravel",
    "license": "MIT",
    "keywords": ["laravel", "multitenancy", "tenancy"],
    "authors": [
        {
            "name": "Yanhao Li",
            "email": "hi@yanhaoli.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Yanhaoli\\OfficeBuilding\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Yanhaoli\\OfficeBuilding\\Providers\\OfficeBuildingServiceProvider"
            ],
            "aliases": {
                "OfficeBuilding": "Yanhaoli\\OfficeBuilding\\Facades\\OfficeBuilding"
            }
        }
    }
}
